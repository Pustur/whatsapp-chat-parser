{
  "name": "whatsapp-chat-parser",
  "version": "4.0.0",
  "description": "A package to parse WhatsApp chats with Node.js or in the browser ðŸ’¬",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup",
    "test": "TZ=UTC vitest run",
