    "test:coverage": "TZ=UTC vitest run --coverage",
    "lint": "tsc && eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build && npm t"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "whatsapp",
    "chat",
    "discussion",
    "conversation",
    "log",
    "parser"
